    "# Create function called multi_3_5(n) to calculate a sum...\n",
    "# where (i) n is below 10 and (ii) n is below 1,000\n",
    "def multi_3_5(n):\n",
    "    sum = 0\n",
    "    # loop i where range(1, n); excludes 'n' so will iterate everything below 'n'\n",
    "    for i in range(1, n):\n",
    "        if (i % 3 == 0) or (i % 5 == 0):\n",
    "            sum += i\n",
    "    return sum\n",
    "\n",
    "multi_3_5(10)\n",
    "multi_3_5(1000)"
